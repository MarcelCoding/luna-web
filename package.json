    "@types/jasmine": "3.8.2",
    "@types/luxon": "2.0.2",
    "@types/node": "16.7.10",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "7.32.0",
